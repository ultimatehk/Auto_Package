<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE log4j:configuration PUBLIC "-//APACHE//DTD LOG4J 1.2//EN" "log4j.dtd">
<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">
	<appender name="gyyx" class="org.apache.log4j.FileAppender">
		<param name="File" value="E:/Auto_Package/log.txt" />
		<layout class="org.apache.log4j.PatternLayout">
			<param name="ConversionPattern" value="[GYYX][%d{yyyy/MM/dd HH:mm:ss,SSS}][%-5p]%C - %m%n" />
		</layout>
	</appender>
	<root>
		<level value="all" />
		<appender-ref ref="gyyx" />
	</root>
</log4j:configuration>